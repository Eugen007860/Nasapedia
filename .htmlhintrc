{
  "extends": "html5-lint/configurations/default.json",
  "rules": {
    "void-style": "both",
    "indent-style": "spaces",
    "indent-width": 2,
    "line-end-style": "lf",
    "attr-name-style": "dash",
    "attr-quote-style": "double",
    "tag-name-lowercase": true,
    "attr-lowercase": true,
    "attr-no-duplication": true,
    "attr-value-double-quotes": true,
    "doctype-first": true,
    "doctype-html5": true,
    "focusable-tabindex-style": "static",
    "id-class-value": "dash",
    "id-no-duplication": true,
    "img-req-alt": true,
    "label-req-for": true,
    "label-no-duplication": true,
    "table-req-caption": true,
    "table-req-header": false,
    "table-req-rowgroup": true,
    "table-req-unique-id": true,
    "a-req-href": true,
    "a-no-external-protocol": true,
    "img-req-src": true,
    "style-disabled": true,
    "script-disabled": true,
    "focusable-tabindex-style": "static",
    "html-req-lang": true,
    "head-req-title": true,
    "head-valid-content-model": true,
    "dl-req-dd-dt": true,
    "void-style": true,
    "alt-require": true,
    "alt-no-duplication": true,
    "img-req-alt": true,
    "label-req-for": true,
    "label-no-duplication": true,
    "input-req-label": true,
    "input-req-placeholder": true,
    "input-valid-type": true,
    "a-req-rel": true,
    "a-valid-content-model": true,
    "table-req-caption": true,
    "table-req-header": true,
    "table-req-rowgroup": true,
    "table-req-unique-id": true,
    "table-valid-content-model": true,
    "a-valid-href": true
  }
}

